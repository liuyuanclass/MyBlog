<h1>Home#index</h1>

<% if current_user.admin? %>
  <%= link_to "Teachers", teachers_path %>
  <%=link_to "Students",students_path %>
  <%=link_to "Admin",ad_path(current_user) %>
<% elsif current_user.student? %>
  <%= link_to "Student", client_path(current_user)%>
<% elsif current_user.teacher? %>
  <%=link_to "Teacher",list_path(current_user) %>
 <% else %>
  <% root_path %>
<% end %>
