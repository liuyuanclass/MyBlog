<%= form_with(model: community, local: true) do |form| %>
  <% if community.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(community.errors.count, "error") %> prohibited this community from being saved:</h2>

      <ul>
        <% community.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <p>社团的名称</p>
    <%= form.text_field :community_id %>
  </div>

  <div class="field">
    <p>社长ID</p>
    <%= form.text_field :leader  %>
  </div>

  <div class="field">
    <p>这是一个怎样的社团呢</p>
    <%= form.text_field :describe %>
  </div>
  <br>
  <div class="actions">
    <%= form.submit "创建社团" %>
  </div>
<% end %>
